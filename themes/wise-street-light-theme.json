    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.quoted.docstring.multi.python",
                "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
                "string.quoted.docstring.multi.python punctuation.definition.string.end.python"
            ],
            "settings": {
                "foreground": "#9ca3af",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string.quoted",
                "string.template",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#0F7B6C"
            }
        },
        {
            "name": "Punctuation within templates",
            "scope": "string.template meta.embedded.line",
            "settings": {
                "foreground": "#5c5c5c"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "name": "Language variable",
            "scope": "variable.language",
            "settings": {
                "foreground": "#db4b4b",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Parameter",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#000000",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Storage (declaration or modifier keyword)",
            "scope": [
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#783688"
            }
        },
        {
            "name": "Constant",
            "scope": "constant",
            "settings": {
                "foreground": "#e66f33"
            }
        },
        {
            "name": "Regex",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#963889"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#e66f33"
            }
        },
        {
            "name": "Language constant (boolean, null)",
